    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-module-resolver": "^1.5.0",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "repository": {
    "type": "git",

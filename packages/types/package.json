{
  "name": "@yetiwallet/types",
  "version": "0.0.4",
  "description": "typescript types for yeti packages",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
